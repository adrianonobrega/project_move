generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Movie {
  id          String   @id @default(uuid())
  title       String
  description String?
  coverUrl    String?
  rating      Float?
  duration    Int?
  folderPath  String
  hlsManifest String
  createdAt   DateTime @default(now())
}